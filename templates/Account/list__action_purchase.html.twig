{% if object.accountTypeName in ['Cash', 'Debit Card', 'Credit Card'] %}
    {% if object.deactivated == false %}
        <a class="btn btn-sm btn-default" 
            style="margin: 0 2px 0 0;"
            href="{{ admin.generateObjectUrl('redirectToPurchase', object) }}" 
            title="Purchase"
        >
            <i class="fa fa-shopping-cart" aria-hidden="true"></i>
        </a>
    {% endif %}
{% endif %}